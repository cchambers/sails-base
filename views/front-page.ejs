<div class="contain">
  <div class="row">
    <div class="col-2-3">
      <div class="media primary" style="background-image:url(<%= data.images[0].media %>)" title="<%= data.images[0].title %>">
      </div>
    </div>
    <div class="col-1-3">
      <ul class="text-entries">
       <% for (var x = 0; x < data.text.length; x++) { %>
       <li><%= data.text[x].title %></li>
       <% } %>
       <% for (var x = 0; x < data.links.length; x++) { %>
       <li><%= data.links[x].title %></li>
       <% } %>
     </ul>
   </div>
 </div>
 <div class="row tertiary-media">
  <div class="col-1-2 images flex">
    <% for (var x = 1; x < 9; x++) { %>
    <div class="media image flex-item" style="background-image:url(<%= data.images[x].media %>)"> 
    <a><%= data.images[x].title %></a>
    </div>
    <% } %>
  </div>

  <div class="col-1-2">
    <div class="videos flex">
      <%
      for (var x = 0; x < 9; x++) { 
        var media = data.videos[x].media;
        if(media.indexOf('youtu') > 0) {
          var which = "" 
          if (media.indexOf('watch?v') > 0) { 
            which = media.split("=")[1] 
          } else if (media.indexOf('youtu.be') > 0) { 
            which = media.split("/").pop() 
          } else if (media.indexOf('embed') > 0) { 
            which = media.split("/").pop().split("?")[0] 
          } 
          if (which.indexOf('?') > 0) {
            which = which.split('?')[0]
          } 


          if (which != "") { %> 
          <div class="media flex-item" style="background-image:url(http://img.youtube.com/vi/<%= which %>/0.jpg)">
            <a href="/sub/<%= data.videos[x].postedTo.slug %>/<%= data.videos[x].slug %>"><%= data.videos[x].title %></a>
          </div>
          <% }
        } 
      } %>
    </div>
  </div>
</div>
