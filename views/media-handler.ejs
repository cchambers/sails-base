<% if (entry.media) { %>
  <% if (entry.media.indexOf('imgur') > 0) { %>
  <div class="media imgur" style="background-image:url(<%= entry.media %>)"><img src="<%= entry.media %>"></div>
  <% } %> 

  <% if (entry.media.indexOf('gfycat') > 0) { %>
  <% var url = "" %>
  <div class="media gfycat" style="height: 40rem">
    <video id="sampleMovie" loop autoplay src="http://zippy.gfycat.com/OrdinarySlipperyGalapagossealion.webm" controls></video>
  </div>

  <% } %> 

  <% if (entry.media.indexOf('youtube') > 0) { %>
  <div class="media youtube">
    <% if (entry.media.indexOf('watch?v') > 0) { %>
    <% var which = entry.media.split("=")[1] %>
    <iframe width="640" height="360" src="https://www.youtube.com/embed/<%= which %>" frameborder="0" allowfullscreen></iframe>
    <% } %> 

    <% if (entry.media.indexOf('embed') > 0) { %>
    <iframe width="640" height="360" src="<%= entry.media %>" frameborder="0" allowfullscreen></iframe>
    <% } %> 

  </div>
  <% } %> 

<% } else { console.log("no media") } %> 

